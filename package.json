{
  "name": "@anasyakubu/node-kickstart",
  "version": "1.0.5",
  "private": false,
  "description": "A CLI tool for scaffolding a Node.js application with TypeScript, Express.js, Dotenv, CORS, Mongoose (MongoDB), Axios, and more, following the MVC (Model-View-Controller) architecture and industry-standard best practices.",
  "main": "build/index.js",
  "bin": {
    "node-kickstart": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli", "nodejs", "express", "mvc", "boilerplate", "typescript", "scaffolding", "kickstart", "anasyakubu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anasyakubu/node-kickstart"
  },
  "author": "Anas Yakubu",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@types/yargs": "^17.0.33",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "yargs": "^18.0.0"
  }
}
